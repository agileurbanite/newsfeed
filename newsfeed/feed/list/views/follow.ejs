<div class="feed" <%= (el) -> el.data('feed', current) %>>
  <div class="profile">
    <!--img src="/images/<%= current.attr('follow.user.username') %>" alt="Profile Picture" /-->
    <img src="http://placehold.it/50x50" />
  </div>
  <div class="post">
    <p><span class="user"><a href="<%= current.attr('follow.user.username') %>"><%= current.attr('follow.user.fullname') %></a></span> is now following <span class="user"><a href="<%= current.attr('follow.following.username') %>"><% if (current.attr('follow.following.fullname')) { %><%= current.attr('follow.following.fullname') %><% } else { %><%= current.attr('follow.following.username') %><% } %></a></span></p>
  </div>
</div>